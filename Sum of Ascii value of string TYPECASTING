

#include<bits/stdc++.h>
using namespace std;

int main() {
    
    string S;
    
    
    getline(cin,S);
    
    //cout<<S<<endl;
    
    int sum=0;
    
    for(int i=0;i<S.length();i++)
    {
        sum=sum+S[i];
    }
    
    
    
    
    cout<<"Sum of character - "<<sum<<endl;
        

    
    return 0;
}


