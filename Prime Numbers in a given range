#include<bits/stdc++.h>
using namespace std;

bool isprime(int n)
{
    for(int i=2;i*i<=n;i++)
    {
        if(n%i==0)
        {
            return 0;
        }
    }
    return 1;
}

int main() {
    
    int n;
    cout<<"Enter the lower Range "<<endl;
    cin>>n;
    
    int p;
    cout<<"Enter the upper Range "<<endl;
    cin>>p;
    
    for(int i=n;i<=p;i++)
    {
        if(isprime(i))
        {
            cout<<i<<" ";
        }
    }
    
    return 0;
}


Enter the lower Range 
2
Enter the upper Range 
10
2 3 5 7 

=== Code Execution Successful ===
