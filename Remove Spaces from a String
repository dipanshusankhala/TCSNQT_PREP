#include<bits/stdc++.h>
using namespace std;

int main()
{
    string S;
    getline(cin,S);
    
    string ans;
    
    for(int i=0;i<S.length();i++)
    {
        char ch=S[i];
        if(ch!=' ')
        {
            ans.push_back(ch);
        }
        else
        {
            continue;
        }
    }
    
    cout<<ans<<endl;
    
    return 0;
}

