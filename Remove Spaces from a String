#include<bits/stdc++.h>
using namespace std;

int main()
{
    string S;
    getline(cin,S);
    
   int count=0;
   
   for(int i=0;i<S.length();i++)\
   {
       char ch=S[i];
       
       if(ch!=' ')
       {
           S[count]=S[i];
           count++;
       }
       
       //cout<<count<<endl;
   }
   S.resize(count);
   
   
   
   
   cout<<S<<endl;
    return 0;
}


