Problem Statement: Write a program to remove all characters from a string except alphabets in a given string.

#include<bits/stdc++.h>
using namespace std;

int main()
{
    string S;
    getline(cin,S);
    
    string ans="";
    
    for(int i=0;i<S.length();i++)
    {
        char ch=S[i];
        if((ch>='a' && ch<='z') || (ch>='A' && ch<='Z'))
        {
            ans.push_back(ch);
        }
    }
    
    cout<<ans<<endl;
    
    
    return 0;
}





